{
    "rulesDirectory": [
        "node_modules/codelyzer",
        "node_modules/tslint-eslint-rules/dist/rules"
    ],

    "rules": {
        "member-access": false,
        "member-ordering": [
            true,
            "public-before-private",
            "static-before-instance",
            "variables-before-functions"
        ],
        "no-any": true,
        "no-inferrable-types": false,
        "no-internal-module": true,
        "no-namespace": false,
        "no-reference": true,
        "no-var-requires": true,
        "only-arrow-functions": true,
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "arrow-parameter",
            "property-declaration",
            "variable-declaration",
            "member-variable-declaration"
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            },
            {
                "call-signature": "onespace",
                "index-signature": "onespace",
                "parameter": "onespace",
                "property-declaration": "onespace",
                "variable-declaration": "onespace"
            }
        ],

        "ban": false,
        "curly": true,
        "forin": true,
        "label-position": true,
        "label-undefined": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-key": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-invalid-this": false,
        "no-null-keyword": true,
        "no-shadowed-variable": true,
        "no-string-literal": true,
        "no-switch-case-fall-through": true,
        "no-unreachable": true,
        "no-unsafe-finally": true,
        "no-unused-expression": true,
        "no-unused-new": true,
        "no-unused-variable": [
            true,
            "check-parameters",
            {
                "ignore-pattern": "e|event"
            }
        ],
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "radix": true,
        "switch-default": true,
        "triple-equals": true,
        "use-isnan": true,
        "use-strict": [
            true,
            "check-module"
        ],

        "eofline": true,
        "indent": [
            true,
            "spaces"
        ],
        "linebreak-style": [
            true,
            "LF"
        ],
        "max-file-line-count": false,
        "max-line-length": [
            true,
            120
        ],
        "no-default-export": true,
        "no-mergeable-namespace": true,
        "no-require-imports": false,
        "no-trailing-whitespace": true,
        "object-literal-sort-keys": true,
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],

        "align": [
            true,
            "parameters",
            "arguments"
        ],
        "arrow-parens": true,
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "interface-name": [
            true,
            "always-prefix"
        ],
        "jsdoc-format": false,
        "new-parens": true,
        "no-angle-bracket-type-assertion": false,
        "no-consecutive-blank-lines": false,
        "no-constructor-vars": false,
        "object-literal-key-quotes": [
            true,
            "as_needed"
        ],
        "one-line": [
            true,
            "check-open-brace",
            "check-catch",
            "check-else",
            "check-finally",
            "check-whitespace"
        ],
        "one-variable-per-declaration": [
            true,
            "ignore-for-loop"
        ],
        "ordered-imports": [
            true,
            "lowercase-last"
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore",
            "ban-keywords"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-type"
        ],

        "directive-selector-name": [
            true,
            "camelCase"
        ],
        "component-selector-name": [
            true,
            "kebab-case"
        ],
        "directive-selector-type": [
            true,
            "attribute"
        ],
        "component-selector-type": [
            true,
            "element"
        ],
        "directive-selector-prefix": [
            true,
            "lb"
        ],
        "component-selector-prefix": [
            true,
            "lb"
        ],
        "use-input-property-decorator": true,
        "use-output-property-decorator": true,
        "use-host-property-decorator": true,
        "no-attribute-parameter-decorator": true,
        "no-input-rename": true,
        "no-output-rename": true,
        "no-forward-ref": true,
        "use-life-cycle-interface": true,
        "use-pipe-transform-interface": true,
        "pipe-naming": [
            true,
            "camelCase",
            ""
        ],
        "component-class-suffix": true,
        "import-destructuring-spacing": true,

        "no-constant-condition": [
            true,
            {
                "checkloops": false
            }
        ],
        "no-duplicate-case": true,
        "no-ex-assign": false,
        "no-extra-boolean-cast": true,
        "no-extra-semi": true,
        "no-inner-declarations": true,
        "no-invalid-regexp": true,
        "no-irregular-whitespace": true,
        "no-sparse-arrays": true,
        "no-unexpected-multiline": true,
        "valid-jsdoc": [
            true,
            {
                "prefer": {
                    "arg": "param",
                    "Arg": "param",
                    "argument": "param",
                    "Argument": "param",
                    "Param": "param",
                    "returns": "return",
                    "Returns": "return",
                    "virtual": "abstract",
                    "Virtual": "abstract",
                    "Abstract": "abstract",
                    "Access": "access",
                    "Alias": "alias",
                    "augments": "extends",
                    "Augments": "extends",
                    "Extends": "extends",
                    "Author": "author",
                    "Borrows": "borrows",
                    "Callback": "callback",
                    "class": "constructor",
                    "Class": "constructor",
                    "Constructor": "constructor",
                    "Classdesc": "classdesc",
                    "Constant": "constant",
                    "const": "constant",
                    "Const": "constant",
                    "Constructs": "constructs",
                    "construct": "constructs",
                    "Construct": "constructs",
                    "Copyright": "copyright",
                    "defaultvalue": "default",
                    "Defaultvalue": "default",
                    "Default": "default",
                    "defaults": "default",
                    "Defaults": "default",
                    "Deprecated": "deprecated",
                    "Description": "description",
                    "desc": "description",
                    "Desc": "description",
                    "Enum": "enum",
                    "Event": "event",
                    "Example": "example",
                    "exemple": "example",
                    "Exemple": "example",
                    "Exports": "exports",
                    "export": "exports",
                    "Export": "exports",
                    "External": "external",
                    "host": "external",
                    "Host": "external",
                    "File": "file",
                    "fileoverview": "file",
                    "fileOverview": "file",
                    "Fileoverview": "file",
                    "FileOverview": "file",
                    "overview": "file",
                    "Overview": "file",
                    "filedesc": "file",
                    "fileDesc": "file",
                    "Filedesc": "file",
                    "FileDesc": "file",
                    "Emits": "emits",
                    "emit": "emits",
                    "emits": "emits",
                    "fires": "emits",
                    "Fires": "emits",
                    "fire": "emits",
                    "Fire": "emits",
                    "Function": "function",
                    "func": "function",
                    "Func": "function",
                    "Method": "method",
                    "Global": "global",
                    "Ignore": "ignore",
                    "Implements": "implements",
                    "implement": "implements",
                    "Implement": "implements",
                    "Inheritdoc": "inheritdoc",
                    "inheritDoc": "inheritdoc",
                    "InheritDoc": "inheritdoc",
                    "inherit": "inheritdoc",
                    "Inherit": "inheritdoc",
                    "inherits": "inheritdoc",
                    "Inherits": "inheritdoc",
                    "Inner": "inner",
                    "Instance": "instance",
                    "Interface": "interface",
                    "Kind": "kind",
                    "Lends": "lends",
                    "lend": "lends",
                    "Lend": "lends",
                    "License": "license",
                    "licence": "license",
                    "Licence": "license",
                    "Listens": "listens",
                    "listen": "listens",
                    "Listen": "listens",
                    "Member": "member",
                    "var": "member",
                    "Var": "member",
                    "Memberof": "memberof",
                    "memberOf": "memberof",
                    "MemberOf": "memberof",
                    "Mixes": "mixes",
                    "mix": "mixes",
                    "Mix": "mixes",
                    "mixe": "mixes",
                    "Mixe": "mixes",
                    "Mixin": "mixin",
                    "Module": "module",
                    "Name": "name",
                    "Namespace": "namespace",
                    "nameSpace": "namespace",
                    "NameSpace": "namespace",
                    "Override": "override",
                    "overrides": "override",
                    "Overrides": "override",
                    "Private": "private",
                    "priv": "private",
                    "Priv": "private",
                    "Property": "property",
                    "prop": "property",
                    "Prop": "property",
                    "props": "property",
                    "Props": "property",
                    "propertie": "property",
                    "Propertie": "property",
                    "Protected": "protected",
                    "prot": "protected",
                    "Prot": "protected",
                    "Public": "public",
                    "Pub": "public",
                    "pub": "public",
                    "Readonly": "readonly",
                    "readOnly": "readonly",
                    "ReadOnly": "readonly",
                    "ro": "readonly",
                    "Requires": "requires",
                    "require": "requires",
                    "Require": "requires",
                    "See": "see",
                    "seealso": "see",
                    "Seealso": "see",
                    "seeAlso": "see",
                    "SeeAlso": "see",
                    "also": "see",
                    "Also": "see",
                    "Since": "since",
                    "Static": "static",
                    "stat": "static",
                    "Stat": "static",
                    "Summary": "summary",
                    "sum": "summary",
                    "Sum": "summary",
                    "summarize": "summary",
                    "Summarize": "summary",
                    "This": "this",
                    "Exception": "exception",
                    "excep": "exception",
                    "Excep": "exception",
                    "except": "exception",
                    "Except": "exception",
                    "throws": "exception",
                    "Throws": "exception",
                    "throw": "exception",
                    "throws": "exception",
                    "Todo": "todo",
                    "toDo": "todo",
                    "ToDo": "todo",
                    "Tutorial": "tutorial",
                    "tuto": "tutorial",
                    "Tuto": "tutorial",
                    "tut": "tutorial",
                    "Tut": "tutorial",
                    "Type": "type",
                    "Typedef": "typedef",
                    "typeDef": "typedef",
                    "TypeDef": "typedef",
                    "Variation": "variation",
                    "variant": "variation",
                    "Variant": "variation",
                    "Version": "version",
                    "vers": "version",
                    "Vers": "version",
                    "ver": "version",
                    "Ver": "version",
                    "v.": "version",
                    "V.": "version",
                    "Link": "link"
                },
                "preferType": {
                    "object": "Object",
                    "String": "string",
                    "Number": "number",
                    "integer": "number",
                    "Integer": "number",
                    "Float": "number",
                    "float": "number",
                    "Function": "function",
                    "date": "Date",
                    "Any": "*",
                    "any": "*",
                    "Mixed": "*",
                    "mixed": "*",
                    "Various": "*",
                    "various": "*",
                    "Misc": "*",
                    "misc": "*",
                    "observable": "Observable",
                    "Boolean": "boolean",
                    "Bool": "boolean",
                    "bool": "boolean"
                },
                "requireReturn": false,
                "requireReturnType": true,
                "requireParamDescription": true,
                "requireReturnDescription": true,
                "matchDescription": "^[/sA-Z0-9][\\s\\S]*[.]$"
            }
        ],
        "valid-typeof": true,
        "array-bracket-spacing": [
            true,
            "never"
        ]
    }
}
