{
    "name": "lumboilerplate",
    "version": "0.0.1",
    "description": "Default boilerplate for new project generation",
    "keywords": ["boilerplate", "project", "lumapps"],
    "homepage": "https://github.com/lumapps/lumboilerplate/",
    "main": "setup.sh",
    "config": {},
    "scripts": {
        "build": "npm run -s build:dev",
        "build:dev": "npm run -s task build:dev",
        "build:prod": "npm run -s task build:prod",
        "dev": "npm run -s build:dev",
        "dist": "npm run -s build:prod",
        "docs": "npm run -s task docs",
        "prod": "npm run -s build:prod",
        "lint": "npm run -s task lint",

        "lang": "npm run -s task lang",
        "langs": "npm run -s lang",

        "e2e": "npm run -s task e2e",
        "e2e:fast": "npm run -s task e2e:fast",
        "e2e:debug": "npm run -s task e2e:debug",
        "coverage": "npm run -s task coverage",
        "unitest": "npm run -s task unitest",
        "unitest:debug": "npm run -s task unitest:debug",
        "unitest:live": "npm run -s task unitest:live",
        "unitest:live:debug": "npm run -s task unitest:live:debug",
        "tests": "npm run -s task tests",
        "tests:debug": "npm run -s task tests:debug",
        "test": "npm run -s unitest",
        "test:debug": "npm run -s unitest:debug",
        "test:live": "npm run -s unitest:live",
        "test:live:debug": "npm run -s unitest:live:debug",

        "clean": "npm run -s task clean:dist",
        "clean:all": "npm run -s task clean:all",
        "clean:unit:report": "npm run -s task clean:unit:report",
        "clean:e2e:report": "npm run -s task clean:e2e:report",
        "clean:docs": "npm run -s task clean:docs",

        "help": "npm run -s task -- --tasks",
        "task": "gulp",
        "tasks": "help",

        "live": "npm run -s start",
        "start": "npm run -s serve:live",
        "serve": "npm run -s task serve",
        "serve:live": "npm run -s task serve:live",
        "serve:prod": "npm run -s task serve:prod",

        "http-server": "http-server",
        "istanbul": "istanbul",
        "karma": "karma",
        "protractor": "protractor",
        "rimraf": "rimraf",
        "remap-istanbul": "remap-istanbul",
        "tsc": "tsc",
        "tsconfig-lint": "tsconfig-lint",
        "tslint": "tslint",
        "typedoc": "typedoc",

        "webpack": "webpack",
        "webpack-dev-server": "webpack-dev-server",

        "webdriver-manager": "webdriver-manager",
        "webdriver:start": "npm run -s webdriver-manager start 2>&1",
        "webdriver:update": "npm run -s webdriver-manager update 2>&1"
    },
    "engines": {
        "node": ">= 4.4.7",
        "npm": ">= 3"
    },
    "private": true,
    "author": "Lumapps <contact@lumapps.com> (https://www.lumapps.com)",
    "contributors": [
        "Cl√©ment P. <clement@lumapps.com> (https://github.com/clementprevot)"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/lumapps/lumboilerplate.git"
    },
    "bugs": {
        "url": "https://github.com/lumapps/lumboilerplate/issues"
    },
