{
    "name": "lumboilerplate",
    "version": "1.0.0",
    "description": "Default boilerplate for new project generation",
    "keywords": [
        "angular2",
        "boilerplate",
        "lumapps",
        "project",
        "typescript",
        "sass",
        "webpack"
    ],
    "homepage": "https://github.com/lumapps/lumboilerplate/",
    "main": "setup.sh",
    "config": {},
    "scripts": {
        "build": "npm run -s build:dev",
        "prebuild:dev": "npm run -s lint:src && npm run -s clean:dist",
        "build:dev": "npm run -s task -- build:dev",
        "prebuild:prod": "npm run -s lint:src && npm run -s clean:dist",
        "build:prod": "npm run -s task -- build:prod",
        "dev": "npm run -s build:dev",
        "dist": "npm run -s build:prod",
        "docs": "npm run -s task -- docs",
        "prod": "npm run -s build:prod",

        "lint": "npm run -s lint:src",
        "lint:all": "npm run -s lint:config && npm run -s lint:src",
        "lint:config": "npm run -s task -- lint:config",
        "lint:src": "npm run -s task -- lint:src",
        "lint:src:ts": "npm run -s task -- lint:src:ts",
        "lint:src:js": "npm run -s task -- lint:src:js",
        "lint:src:scss": "npm run -s task -- lint:src:scss",

        "lang": "npm run -s task -- lang",
        "langs": "npm run -s lang",

        "pree2e": "npm run -s serve:prod",
        "e2e": "npm run -s task -- e2e",
        "pree2e:fast": "npm run -s serve:prod",
        "e2e:fast": "npm run -s task -- e2e:fast",
        "pree2e:debug": "npm run -s serve:prod",
        "e2e:debug": "npm run -s task -- e2e:debug",
        "pree2e:phantom": "npm run -s serve:prod",
        "e2e:phantom": "npm run -s task -- e2e:phantom",
        "pree2e:phantom:fast": "npm run -s serve:prod",
        "e2e:phantom:fast": "npm run -s task -- e2e:phantom:fast",
        "pree2e:hidden": "npm run -s serve:prod",
        "e2e:hidden": "npm run -s e2e:phantom",
        "pree2e:hidden:fast": "npm run -s serve:prod",
        "e2e:hidden:fast": "npm run -s e2e:phantom:fast",

        "unit": "npm run -s task -- unit",
        "unit:debug": "npm run -s task -- unit:debug",
        "unit:live": "npm run -s task -- unit:live",
        "unit:live:debug": "npm run -s task -- unit:live:debug",
        "tests": "npm run -s task -- tests",
        "tests:debug": "npm run -s task -- tests:debug",
        "test": "npm run -s unit",
        "test:debug": "npm run -s unit:debug",
        "test:live": "npm run -s unit:live",
        "test:live:debug": "npm run -s unit:live:debug",

        "clean": "npm run -s clean:dist",
        "clean:all": "npm run -s task -- clean:all",
        "clean:dist": "npm run -s task -- clean:dist",
        "clean:docs": "npm run -s task -- clean:docs",
        "clean:packages": "npm run -s task -- clean:packages",
        "clean:npm": "npm run -s clean:packages",
        "clean:project": "npm run -s task -- clean:project",
        "clean:reports": "npm run -s clean:tests:reports",
        "clean:tests": "npm run -s clean:tests:reports",
        "clean:tests:reports": "npm run -s task -- clean:tests:reports",

        "help": "npm run -s task -- --tasks",
        "task": "gulp",
        "tasks": "help",

        "setup": "rm -Rf -- node_module && npm cache clean && npm install && npm run -s task -- clean:project",

        "commit": "npm run -s task -- commit",
        "co": "npm run -s commit",
        "cz": "npm run -s commit",

        "live": "npm run -s start",
        "start": "npm run -s serve:live",
        "run": "npm run -s serve:live",
        "serve": "npm run -s task -- serve",
        "serve:live": "npm run -s task -- serve:live",
        "preserve:dev": "npm run -s build:dev",
        "serve:dev": "npm run -s task -- serve:prod",
        "preserve:prod": "npm run -s build:prod",
        "serve:prod": "npm run -s task -- serve:prod",

        "commitizen": "git-cz",
        "eslint": "eslint",
        "http-server": "http-server",
        "istanbul": "istanbul",
        "karma": "karma",
        "protractor": "protractor",
        "rimraf": "rimraf",
        "sass-lint": "sass-lint",
        "tsc": "tsc",
        "tsconfig-lint": "tsconfig-lint",
        "tslint": "tslint",
        "typedoc": "typedoc",
        "webdriver-manager": "webdriver-manager",
        "webdriver:start": "npm run -s webdriver-manager start 2>&1",
        "webdriver:update": "npm run -s webdriver-manager update 2>&1",
        "webpack": "webpack",
        "webpack-dashboard": "webpack-dashboard",
        "webpack-dev-server": "webpack-dev-server"
    },
    "engines": {
        "node": ">= 4.4.7",
        "npm": ">= 3"
    },
    "private": true,
    "author": "Lumapps <contact@lumapps.com> (https://www.lumapps.com)",
    "contributors": [
        "Arnaud V. <arnaud@lumapps.com> (https://github.com/arnaudvalle)",
        "Cl√©ment P. <clement@lumapps.com> (https://github.com/clementprevot)"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/lumapps/lumboilerplate.git"
    },
    "bugs": {
        "url": "https://github.com/lumapps/lumboilerplate/issues"
    },
    "dependencies": {
        "@angular/common": "latest",
        "@angular/compiler": "latest",
        "@angular/core": "latest",
        "@angular/forms": "latest",
        "@angular/http": "latest",
        "@angular/platform-browser": "latest",
        "@angular/platform-browser-dynamic": "latest",
        "@angular/platform-server": "latest",
        "@angular/router": "latest",

        "@ngrx/core": "latest",
        "@ngrx/store": "latest",

        "core-js": "latest",
        "ie-shim": "latest",
        "rxjs": "latest",
        "zone.js": "latest"
    },
    "devDependencies": {
        "commitizen": "latest",
        "http-server": "latest",
        "phantomjs-prebuilt": "latest",
        "rimraf": "latest",

        "gulp": "latest",
        "gulp-cli": "latest",
        "gulp-shelter": "latest",

        "@angularclass/hmr": "latest",

        "autoprefixer": "latest",
        "cssnano": "latest",
        "node-sass": "latest",

        "karma": "latest",
        "karma-chrome-launcher": "latest",
        "karma-coverage": "latest",
        "karma-jasmine": "latest",
        "karma-jasmine-matchers": "latest",
        "karma-mocha-reporter": "latest",
        "karma-phantomjs-launcher": "latest",
        "karma-remap-coverage": "latest",
        "karma-sourcemap-loader": "latest",
        "karma-webpack": "latest",

        "jasmine-expect": "latest",
        "jasmine-spec-reporter": "latest",

        "protractor": "latest",
        "protractor-jasmine2-html-reporter": "latest",

        "codelyzer": "latest",
        "eslint": "latest",
        "sass-lint": "latest",
        "tsconfig-lint": "latest",
        "tslint": "latest",
        "tslint-eslint-rules": "latest",
        "ts-helpers": "latest",
        "ts-node": "latest",
        "typedoc": "latest",
        "typescript": "latest",

        "@types/hammerjs": "latest",
        "@types/jasmine": "latest",
        "@types/jasmine-expect": "latest",
        "@types/node": "latest",
        "@types/protractor": "latest",
        "@types/selenium-webdriver": "2.44.26",
        "@types/source-map": "latest",
        "@types/uglify-js": "latest",
        "@types/webpack": "latest",

        "@angularclass/hmr-loader": "latest",
        "angular2-template-loader": "latest",
        "awesome-typescript-loader": "latest",
        "css-loader": "latest",
        "file-loader": "latest",
        "html-loader": "latest",
        "istanbul-instrumenter-loader": "^0.2.0",
        "json-loader": "latest",
        "postcss-loader": "latest",
        "resolve-url-loader": "latest",
        "sass-loader": "latest",
        "source-map-loader": "latest",
        "string-replace-loader": "latest",
        "style-loader": "latest",
        "to-string-loader": "latest",
        "tslint-loader": "latest",

        "assets-webpack-plugin": "latest",
        "autopolyfiller-webpack": "latest",
        "copy-webpack-plugin": "latest",
        "extract-text-webpack-plugin": "latest",
        "html-webpack-plugin": "latest",
        "open-browser-webpack-plugin": "latest",
        "sasslint-webpack-plugin": "latest",
        "webpack": "latest",
        "webpack-dashboard": "latest",
        "webpack-dev-middleware": "latest",
        "webpack-dev-server": "latest",
        "webpack-md5-hash": "latest",
        "webpack-merge": "latest",
        "webpack-notifier": "latest"
    }
}
